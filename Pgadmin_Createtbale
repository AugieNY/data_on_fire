SQL Query to create table in PGadmin

CREATE TABLE bank_churn(
CLIENTNUM INT,
Attrition_Flag CHAR(50),
Customer_Age INT,
Gender CHAR(50),
Dependent_count INT,
Education_Level	CHAR(50),
Marital_Status	CHAR(50),
Income_Category	CHAR(50),
Card_Category	CHAR(50),
Months_on_book	INT,
Total_Relationship_Count INT,	
Months_Inactive_12_mon	INT,
Contacts_Count_12_mon	INT,
Credit_Limit FLOAT,	
Total_Revolving_Bal	FLOAT,
Avg_Open_To_Buy	FLOAT,
Total_Amt_Chng_Q4_Q1 FLOAT,
Total_Trans_Amt	FLOAT,
Total_Trans_Ct	FLOAT,
Total_Ct_Chng_Q4_Q1	FLOAT,
Avg_Utilization_Ratio	FLOAT
)
